{
    "name": "nenglish-security/fusion-checker",
    "description": "This package ensures untrusted composer packages are not used.",
    "keywords": [
        "dependency-checker",
        "php71-component",
        "php72-component",
        "secure-by-default"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish6657@gmail.com",
            "homepage": "https://github.com/Nenglish7",
            "role": "Develop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nenglish7\\FusionChecker\\": "./src"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "suggest": {
        "roave/security-advisories": "This package ensures that your application doesn't have installed dependencies with known security vulnerabilities."
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
